{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js",
      "headers": {
        "Content-Security-Policy": "default-src 'none'; script-src 'self' 'unsafe-inline'; font-src 'self' <font-source>;"
      }
    }
  ]
}
